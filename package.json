{
  "name": "placemarkcore",
  "version": "2.0.0",
  "description": "A point of interest web application",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "lint": "eslint . --ext .js",
    "test": "mocha --ui tdd test/**/*.js",
    "testmodels": "mocha --ui tdd test/models/**/*.js",
    "testapi": "mocha --ui tdd test/api/**/*.js",
    "dev": "nodemon src/server.js --ignore src/models/json/*.json --ignore src/models/json/*.tmp"
  },
  "engines": {
    "node": "16.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Berncat/PlaceMarkCore.git"
  },
  "keywords": [
    "poi"
  ],
  "author": "Bernard Cattigan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Berncat/PlaceMarkCore/issues"
  },
  "homepage": "https://github.com/Berncat/PlaceMarkCore#readme",
  "devDependencies": {
    "axios": "^0.26.1",
    "chai": "*",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "mocha": "*",
    "prettier": "^2.6.0"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "@hapi/cookie": "^11.0.2",
    "@hapi/hapi": "^20.2.1",
    "@hapi/vision": "^6.1.0",
    "dotenv": "^16.0.0",
    "handlebars": "^4.7.7",
    "joi": "^17.6.0",
    "lowdb": "^3.0.0",
    "uuid": "^8.3.2"
  }
}
